<h1>Test questions "<%= pp @test.title %>".</h1>
<p><%= link_to 'New question', new_test_question_url %></p>
<table>
  <tr>
    <th>Title</th>
    <th colspan="3">Action</th>
  </tr>

  <% @questions.each do |question| %>
    <tr>
      <td><%= question.title %></td>
      <td><%= link_to 'Delete', question_path(question), data: { "turbo-method": :delete } %></td>
      <td><%= link_to 'Edit', edit_question_path(question) %></td>
    </tr>
  <% end %>
</table>
